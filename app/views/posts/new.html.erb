<%= form_for :post, url: posts_path do |f| %>
<h1>New Student</h1>

<%= form_for :post do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :nickname %><br>
    <%= f.text_area :nickname %>
  </p>

  <p>
    <%= f.label :email %><br>
    <%= f.text_area :email %>
  </p>

  <p>
    <%= f.label :imageurl %><br>
    <%= f.text_area :imageurl %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
  <% end %>
<% end %>
