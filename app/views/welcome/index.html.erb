
<h1>Student</h1>
<%= link_to 'Create Student', new_post_path %> 
<table>
  <tr>
    <th>Name</th>
  
    
    <th>Nickname</th>
  
    
    <th>Email</th>
   
 
    
    <th>Image</th>  
    
  </tr>
 
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.name %></td>
  
      <td><%= post.nickname %></td>
  
      <td><%= post.email %></td>
  
      <td><img src=<%= post.imageurl %> height="200" width="180"></td>
       
    
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    </tr>
  <% end %>
 